separator_block_width=20
separator=false
markup=none

[timew]
command=$HOME/.config/i3blocks/timew.sh
interval=5
signal=1

[calendar]
command=$HOME/.config/i3blocks/calendar.sh
interval=600

[music]
command=$HOME/.config/i3blocks/mpd.sh
interval=persist

[updates]
command=$HOME/.config/i3blocks/updates.sh
interval=2550

[wifi]
command=$HOME/.config/i3blocks/wifi.sh
interval=25

[volume]
command=$HOME/.config/i3blocks/volume.sh
interval=once
signal=1

[battery]
command=$HOME/.config/i3blocks/battery.sh
interval=30

[time]
command=echo "$(date '+%a, %b %d %l:%M%P')    "
interval=11
