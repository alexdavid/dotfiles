separator_block_width=30
markup=none

[volume]
label=
command=$HOME/.config/i3blocks/volume.sh
interval=once
signal=1

[updates]
label=
command=$HOME/.config/i3blocks/updates.sh
interval=600

[date]
label=
command=echo $(date '+%a, %b %d')
interval=600

[time]
label=
command=echo $(date '+%l:%M%P')
interval=10
