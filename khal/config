[calendars]
  [[personal]]
  path = ~/.local/share/calendars/personal/*
  type = discover

  [[work]]
  path = ~/.local/share/calendars/work
  type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
